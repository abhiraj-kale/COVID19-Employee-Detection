VALUES (10,'HUMAN RESOURCE');

UPDATE ZONE_SELECT
SET PERCENTAGE = (
SELECT PERCENTAGE
FROM SYMPTOMS
WHERE SYMPTOMS.USER_ID = ZONE_SELECT.USER_ID
);

