INSERT INTO SUSPECTS(USER_ID,DEPT)
VALUES (8,'PRODUCTION');
INSERT INTO SUSPECTS(USER_ID,DEPT)
VALUES (9,'MARKETING');
INSERT INTO SUSPECTS(USER_ID,DEPT)
VALUES (10,'HUMAN RESOURCE'); 

CALL SET_ZONES;

UPDATE SUSPECTS S
SET SUSPECT='Y' WHERE 
EXISTS ( SELECT 1 FROM ZONE_SELECT
WHERE ZONE='R' AND DEPT=S.DEPT AND S.USER_ID<>USER_ID
);

UPDATE SYMPTOMS
SET PERCENTAGE = CONCAT(PERCENTAGE,' %');
UPDATE ZONE_SELECT
SET PERCENTAGE = CONCAT(PERCENTAGE,' %');